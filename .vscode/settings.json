{
    "workbench.editorAssociations": {
        "*.efi": "hexEditor.hexedit",
        "*.EFI": "hexEditor.hexedit"
    },
    "files.exclude": {
        "**/*.o": true,
        "**/*.d": true,
        "**/*.bin": true,
        "**/*.elf": true,
    },
    "files.associations": {
        "new": "cpp",
        "*.tcc": "cpp",
        "numeric": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "memory_resource": "cpp",
        "vector": "cpp"
    },
}